{
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "no-debugger": 0,
    "no-console": 0,
    "no-undef": 2,
    "new-cap": 0,
    "strict": 0,
    "semi": 2,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": [ 2, "single" ]
  }
}
